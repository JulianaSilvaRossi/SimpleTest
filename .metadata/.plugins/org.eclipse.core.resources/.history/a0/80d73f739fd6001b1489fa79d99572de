package com.automacao.support;

import org.openqa.selenium.WebDriver;

public class Inicio {
	
	String url = "https://www.magazineluiza.com.br/";
	
	private static ThreadLocal<WebDriver> threadDriver = new ThreadLocal<WebDriver>() {
		@Override
		protected synchronized WebDriver initialValue() {
			return abrirChrome();
		}
	};
	
	public static WebDriver criarMultiplusNavegadores() {
		return threadDriver.get();
		
	}

	public static WebDriver abrirChrome() {
		System.setProperty("webdriver.chrome.driver", "./\\drivers\\chrome\\");
		return null;
	}

}
